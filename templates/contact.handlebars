    <div class="modal__btn">
        <button type="button" id="openModalBtn" class="contact_btn">Kontakta oss</button>
    </div>
    <!--Modal Form-->

    <div id="getModal" class="modal">
        <div class="modal__content">
            <span class="close">&times;</span>
            <form id="contact-form">
                <input type="hidden" name="contact_number">
                <label class="user__name">Name</label>
                <input type="text" name="user_name">
                <label class="user__email">Email</label>
                <input id="email" type="email" name="user_email">
                <label class="message">Message</label>
                <textarea id="messageText" name="message"></textarea>
                <input class="modal__button" type="submit" value="Send">
            </form>
            <p id="result"></p>
        </div>
    </div>
    <div class="spaceFooter"></div>
    <div id="pinnedMap"></div>


</body>

